<%= simple_form_for(@app_setting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :app_name %>
    <%= f.input :tab_name %>
    <%= f.input :email %>
    <%= f.input :street %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :postal_code %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
